{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.fixAll.stylelint": "explicit"
	},
	"eslint.nodePath": "./node_modules/linter-bundle/node_modules/eslint",
	"eslint.options": {
		"overrideConfigFile": "./eslint.config.mjs"
	  },
	"[markdown]": {
		"editor.rulers": [300]
	},
	"[javascript]": {
		"editor.rulers": [300]
	}
}
